generator client {
  provider = "prisma-client-js"
  binarytargets = ["native"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model arrest_info {
  id                  Int    @id @default(autoincrement())
  incident_number     Int
  offense_code        Int?
  offense_description String?
  district            String?
  reporting_area      String?
  shooting            Int?
  occurred_on_date    DateTime?
  year                Int?
  month               Int?
  day_of_week         String?
  hour                Int?
  ucr_part            String?
  street              String?
  location            String?
}

model forfeiture_data {
  id              Int    @id @default(autoincrement())
  url             String?
  case_number     String
  court_name      String?
  date            String?
  amount          Float?
  motor_vehicle   String?
  cases_incidents String?
}

model officermisconduct {
  id                Int    @id @default(autoincrement())
  column1           Int?
  ia_no             String
  incident_type     String?
  received_date     DateTime?
  title_rank_snap   String?
  first_name        String?
  last_name         String?
  allegation        String?
  disposition       String?
  completed_date    DateTime?
}

model police_financial {
  id              Int    @id @default(autoincrement())
  employee_id     Int
  name            String?
  department_name String?
  title           String?
  regular         String?
  retro           String?
  other           String?
  overtime        String?
  injured         String?
  detail          String?
  quinn           String?
  total_earnings  String?
  postal          String?
  filename        String?
  year            Int?
}
